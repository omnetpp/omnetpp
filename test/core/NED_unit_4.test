%description:
Test quantities vs unary minus

%file: test.ned

simple Test
{
    parameters:
        @isNetwork(true);
        double p0 @unit(s) = -3s;
        double p1 @unit(s) = -1s200ms; // note: minus sign refers to the quantity as a whole, -(1s200ms)
        double p2 @unit(s) = 1s-200ms; // note: iterpreted as subtraction, not as a single quantity
        double p3 @unit(s) = -1s-200ms; // ditto
        double p4 @unit(dBm) = -10dBm; // logarithmic unit
}

%file: test.cc

#include <omnetpp.h>

using namespace omnetpp;

namespace @TESTNAME@ {

class Test : public cSimpleModule
{
  public:
    Test() : cSimpleModule(16384) { }
    virtual void activity() override;
};

Define_Module(Test);

void Test::activity()
{
#define P(x) EV << #x << "=" << par(#x).doubleValue() << "\n"
    P(p0);
    P(p1);
    P(p2);
    P(p3);
    P(p4);
}

}; //namespace

%contains: stdout
p0=-3
p1=-1.2
p2=0.8
p3=-1.2
p4=-10

