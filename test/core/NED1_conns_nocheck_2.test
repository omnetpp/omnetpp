%description:
Test that check for unconnected gates can be turned off

%file: {}/test.ned

package {};

module Compound
{
    gates:
        output out;
        input in;  // these gates not connected internally
    connections allowunconnected:
}

network Test
{
    submodules:
        a: Compound;
    connections allowunconnected:
}

%exitcode: 0

%contains-regex: stdout
No more events -- simulation ended

