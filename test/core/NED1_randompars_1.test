%description:
Test random module parameters

%file: {}.ned

simple {Simple}
   parameters:
      p0, p1, c0:const, c1:const;
endsimple

network {network}: {Simple}
   parameters:
      p0 = truncnormal(10,3),
      p1 = 1+truncnormal(10,3),
      c0 = exponential(1),
      c1 = 1+exponential(1);
endnetwork

%file: {}.cc

#include <omnetpp.h>

class {Simple} : public cSimpleModule
{
  public:
    {Simple}() : cSimpleModule(16384) { }
    virtual void activity();
};

Define_Module({Simple});

void {Simple}::activity()
{
    for (int i=0; i<params(); i++)
        ev << par(i).name() << ":" << (par(i).isExpression()?0:1) << ":" << par(i).doubleValue() << "," << par(i).doubleValue() << endl;
}

%contains-regex: stdout
p0:0:18\.85\d+,12\.21\d+
p1:0:11\.51\d+,15\.64\d+
c0:1:0\.8746\d+,0\.8746\d+
c1:1:1\.0028\d+,1\.0028\d+
