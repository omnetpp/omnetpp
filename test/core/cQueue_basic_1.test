%description:
check insertions into cQueue

%global:
void dump(cQueue& q) {
    for (cQueue::Iterator it(q); !it.end(); it++)
        ev << " " << it()->getName();
     ev << "\n";
}

%activity:
#define INS(var)   ev<<"INS "<<#var<<": "; cMessage *var=new cMessage(#var); q.insert(var); dump(q);
#define DEL(var)   ev<<"DEL "<<#var<<": "; delete q.remove(var); dump(q);
#define POP()      ev<<"POP:   "; delete q.pop(); dump(q);

cQueue q("q");

INS(d)
DEL(d)
INS(e)
INS(t)
INS(a)
INS(z)
INS(b)
INS(k)
INS(l)
DEL(e)
DEL(k)
DEL(l)
POP()
POP()
POP()
POP()
ev << ".\n";


%contains: stdout
INS d:  d
DEL d:
INS e:  e
INS t:  e t
INS a:  e t a
INS z:  e t a z
INS b:  e t a z b
INS k:  e t a z b k
DEL e:  t a z b k
DEL k:  t a z b
POP:    a z b
POP:    z b
POP:    b
POP:
.
