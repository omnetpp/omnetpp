%description:
Check inheritance for structs

%file: test.msg

namespace {};

struct Base
{
    fields:
        int b;
}

struct MyStruct extends Base
{
    fields:
        char c;
};

%includes:
#include "test_m.h"

%activity:

#define PRINT(X) ev << #X << ":" << X << endl

MyStruct x;

x.b = 11; PRINT(x.b);
x.c = 'x'; PRINT(x.c);

%contains: stdout
x.b:11
x.c:x

