%description:
Test NaN: 0/0 to produce it, and that x!=x if x is NaN

%global:
static bool isNaN(double d) { return d != d; }

%activity:
#define PRINT(x) ev << #x << ":" << (x) << endl;
double zero = 0;
double nan = zero/zero;

PRINT(nan);
PRINT(nan*2);
PRINT(-nan);

PRINT(isNaN(nan));
PRINT(isNaN(3.14));
PRINT(isNaN(1/zero));
PRINT(isNaN(-1/zero));

PRINT(nan==nan);
PRINT(nan!=nan);
PRINT(nan<nan);
PRINT(nan>nan);
PRINT(nan<=nan);
PRINT(nan>=nan);
ev << ".\n";

%contains: stdout
nan:-1.#IND
nan*2:-1.#IND
-nan:1.#QNAN
isNaN(nan):1
isNaN(3.14):0
isNaN(1/zero):0
isNaN(-1/zero):0
nan==nan:0
nan!=nan:1
nan<nan:0
nan>nan:0
nan<=nan:0
nan>=nan:0
.
