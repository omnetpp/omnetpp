%description:
check that backslash line continuation works

%file: test.ned

simple Simple
{
    parameters:
        @isNetwork(true);
        string str;
}

%file: test.cc

#include <omnetpp.h>

USING_NAMESPACE

namespace @TESTNAME@ {

class Simple : public cSimpleModule
{
  public:
    Simple() : cSimpleModule(16384) { }
    virtual void activity();
};

Define_Module(Simple);

void Simple::activity()
{
    EV << "\"" <<(const char *)par("str") << "\"" << endl;
    EV << "." << endl;
}

}; //namespace


%inifile: test.ini
[General]
network = Simple
cmdenv-express-mode = false

*.str = "one \
two  \
three\
    four"
%contains: stdout
"one two  three    four"
.





