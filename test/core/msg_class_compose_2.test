%description:
Check non-cOwnedObject-class as member in non-cOwnedObject class

%file: test.msg

namespace {};

class Class1
{
    int bb;
}


class MyClass extends cOwnedObject
{
    Class1 b;
    char c;
};

%includes:
#include "test_m.h"

%activity:

#define PRINT(X) ev << #X << ":" << X << endl

MyClass x;

x.getB().setBb(11); PRINT(x.getB().getBb());
x.setC('x'); PRINT(x.getC());

%contains: stdout
x.getB().getBb():11
x.getC():x

