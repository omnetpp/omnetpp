%description:
Tests the UnitConversion class

%global:

#include "unitconversion.h"

static void parseAndPrint(const char *text, const char *expectedUnit=NULL)
{
    ev << "'" << text << "'";
    if (expectedUnit) ev << " (as " << expectedUnit << ")";
    ev << " ==> ";
    try {
        double d = UnitConversion::parseQuantity(text, expectedUnit);
        ev << d << "\n";
    } catch (std::exception& e) {
        ev << "ERROR: " << e.what() << "\n";
    }
}

%activity:

const char *expectedUnits[] = {NULL, "s", "ms", "W", "bogomips"};

#define P(x) parseAndPrint(x, expectedUnits[i])

for (int i=0; i<5; i++)
{
    ev << "\n";

    // bad syntax
    P(""); //E
    P("  "); //E
    P("2 33"); //E
    P("2 33 "); //E
    P("2 33s "); //E
    P("1.5s 33"); //E
    P("1s 99 Kbps"); //E
    P("1s 1mi 99 Kbps"); //E
    P("1s 99 bogomips"); //E
    P("3 bogomips 99"); //E
    P("3 bogomips 3 gallons"); //E

    // plain number
    P("3.14");
    P("6e23");
    P(" 3.14 ");

    P("1.5s");
    P(" 1.5s ");
    P(" 1.5 s ");

    P("1500ms");

    P("64 Kbps");
    P("64000 bps");

    P(" 200mW");
    P(" 1W 100mW");

    P("99 bogomips");

    P("1h 0.1s");
    P("1h 2mi 2s 520ms");
    P("1s 550ms 700us ");
    P(" 1s 550ms 700us ");
    P("17s 71ggg");
}
ev << ".\n";


