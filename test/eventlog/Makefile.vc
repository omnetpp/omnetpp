#
# Global definitions
#
!include ..\..\configuser.vc

!if "$(LIB_SUFFIX)"==".dll"
CFLAGS=$(CFLAGS) -DWIN32_DLL
!endif

#
# Local definitions
#
COPTS=$(CFLAGS) -I. -I../../src/common -I../../src/eventlog -I../../include/platdep /Fd"eventlogtest.pdb"

OBJS= eventlogindextest.obj eventlogtest.obj
LIBS= $(OMNETPP_LIB_DIR)/$(TOOLCHAIN_NAME)/eventlogd.lib $(OMNETPP_LIB_DIR)/$(TOOLCHAIN_NAME)/commond.lib

#
# Automatic rules
#
.SUFFIXES : .cc

.cc.obj:
	$(CXX) /Fo"$@" $(COPTS) /c /Tp $<

#
# Targets (TBD: build dll)
#
all: eventlogindextest.exe eventlogtest.exe

eventlogindextest.exe : eventlogindextest.obj $(LIBS)
	$(LINK) $(LDFLAGS) /nologo /out:eventlogindextest.exe eventlogindextest.obj $(LIBS)

eventlogtest.exe : eventlogtest.obj $(LIBS)
	$(LINK) $(LDFLAGS) /nologo /out:eventlogtest.exe eventlogtest.obj $(LIBS)

depend:
	$(MAKEDEPEND) -I. -fMakefile.vc -- *.cc

clean:
	- del *.obj 2> nul
	- del *.ilk *.idb *.pdb *.lib *.exe 2> nul


